pub use .builder.{StrBuilder, ?}

pub fn str.from_utf8_unchecked(bytes: [u8]) -> str =
	unsafe as[_](bytes)

pub fn str.from_utf8_unchecked(bytes: &[u8]) -> &str =
	unsafe as[_](bytes)

pub fn str.from_utf8_mut_unchecked(bytes: &mut [u8]) -> &mut str =
	unsafe as[_](bytes)

pub fn as_bytes(self: &str) -> &[u8] = 
	unsafe as[_](self)
