use std.str.(?)

fn extern "c" write(fd: i32, buf: *u8, count: uint) -> int

let stdout: i32 = 1

fn print*(msg: &str) =
	unsafe write(stdout, msg.ptr, msg.len)

fn print*(msg: str) = print(&msg)

fn println*() = print("\n")
fn println*(msg: &str) = print("{msg}\n")
fn println*(msg: str) = println(&msg)
