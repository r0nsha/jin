import std.fs

fn main() = {
    match std.fs.open("/home/ron/file.txt") {
        Result.Ok(file) -> {
            match file.read-to-end() {
                Result.Ok(array) -> {
                    unsafe std.c.printf("%s".ptr, array.as-ref().ptr)
                }
                Result.Err(err) -> println("Error reading file: {err}")
            }
        }
        Result.Err(err) -> println("Error opening file: {err}")
    }
}
