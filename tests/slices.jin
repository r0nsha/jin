fn extern printf(fmt: *u8, ..) i32

fn main() unit = {
	let a: [int] = []
	let b: [int] = [:3]
	let c = [1, 2, 3, 4, 5]
	print(a)
	print(b)
	print(c)
}

fn print(s: [int]) unit = {
	printf("ptr: %p, cap: %d, len: %d\n".ptr, s.ptr, s.cap, s.len)

	let mut i = 0
	loop if i < s.len {
		printf("yeet\n".ptr)
		i += 1
	}
}
