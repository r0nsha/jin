import std.array.{Array}

fn extern "c" printf(fmt: *u8, ..) i32

fn main() unit = {
	let array = Array.new[int]()
	// array.print()
	print(&array)
}

fn print(s: &Array[int]) unit = {
	printf("[".ptr)
	let mut i = 0
	loop if i < s.len() {
		printf("%d".ptr, s.as_ref().[i])
		if i < s.len() - 1 {
			printf(", ".ptr)
		}
		i += 1
	}
	printf("]\n".ptr)
}
