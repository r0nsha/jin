import std.array.{Array}

fn extern "c" printf(fmt: *u8, ..) i32

fn main() unit = {
	let mut array = Array.new[int]()
	array.print()
}

fn print(array: &Array[int]) unit = {
	let slice = array.as_ref()
	printf("[".ptr)
	let mut i = 0
	loop if i < slice.len {
		printf("%d".ptr, slice.[i])
		if i < slice.len - 1 {
			printf(", ".ptr)
		}
		i += 1
	}
	printf("]\n".ptr)
}
