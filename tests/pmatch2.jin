fn extern "c" printf(fmt: ptr[u8], ..) -> i32

fn main() = {
	match 20 {
		10 | 20 if false -> unsafe printf("guard\n".data)
		10 -> unsafe printf("10\n".data)
		20 -> unsafe printf("20\n".data)
		_ -> unsafe printf("other\n".data)
	}

	match true {
		x if !x -> unsafe printf("guard (x)\n".data) 
		true | false if false -> unsafe printf("guard (or)\n".data)
		true -> unsafe printf("true\n".data)
		false -> unsafe printf("false\n".data)
		_ -> unsafe printf("other\n".data)
	}
}
