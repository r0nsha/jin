fn extern "c" printf(fmt: *U8, ..) -> I32

fn main() = {
	match 20 {
		10 | 20 if false -> unsafe printf("guard\n".ptr)
		10 -> unsafe printf("10\n".ptr)
		20 -> unsafe printf("20\n".ptr)
		_ -> unsafe printf("other\n".ptr)
	}

	match true {
		x if !x -> unsafe printf("guard (x)\n".ptr) 
		true | false if false -> unsafe printf("guard (or)\n".ptr)
		true -> unsafe printf("true\n".ptr)
		false -> unsafe printf("false\n".ptr)
		_ -> unsafe printf("other\n".ptr)
	}
}
