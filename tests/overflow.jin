import std.io.(println)
import std.int.*

fn main() = {
    i()
    println()
    u()
}

fn i() = {
    println("signed:")

    let a: i64 = 1
    let b: i64 = i64_max

    // a + b
    // a - b
    // a * b
}

fn u() = {
    println("unsigned:")

    let a: u8 = 2
    let b: u8 = u8_max

    // a + b
    // a - b
    a * b
}
