import std.io.{println}
import std.int.*

fn main() unit = {
    i()
    println()
    u()
}

fn i() unit = {
    println("signed:")

    let a: i64 = 1
    let b: i64 = I64_MAX

    // a + b
    // a - b
    // a * b
}

fn u() unit = {
    println("unsigned:")

    let a: u8 = 2
    let b: u8 = U8_MAX

    // a + b
    // a - b
    a * b
}
