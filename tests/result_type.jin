fn extern "c" printf(fmt: *u8, ..) -> i32

fn main() = {
	match try_add(1, 2) {
		Ok(res) -> unsafe printf("res = %d\n".ptr, res)
		Err(msg) -> unsafe printf(msg.ptr)
	}
}

fn try_add(a: int, b: int) -> Result[int, str] = {
	if a == b {
		Result.Ok(a + b)
	} else {
		Result.Err("`a` and `b` or not equal!\n")
	}
}
