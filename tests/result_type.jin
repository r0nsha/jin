fn extern printf(fmt: *u8, ..) i32

type Result[t, e] {
	ok(value: t),
	err(error: e)
}

fn main() unit = {
	match try_add(1, 2) {
		Result.ok(res) -> printf("res = %d\n".ptr, res),
		Result.err(msg) -> printf(msg.ptr)
	}
}

fn try_add(a: int, b: int) Result[int, str] = {
	if a == b {
		Result.ok(a + b)
	} else {
		Result.err("`a` and `b` or not equal!\n")
	}
}
