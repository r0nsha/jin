fn extern "c" printf(fmt: ptr[U8], ..) -> I32

fn main() = {
	match try_add(1, 2) {
		Result.ok(res) -> unsafe printf("res = %d\n".data, res)
		Result.err(msg) -> unsafe printf(msg.data)
	}
}

fn try_add(a: Int, b: Int) -> Result[Int, Str] = {
	if a == b {
		Result.ok(a + b)
	} else {
		Result.err("`a` and `b` or not equal!\n")
	}
}
