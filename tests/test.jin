import extern "./foo"

fn extern hello()
let extern answer: int

fn extern exit(status: i32) -> never
fn extern write(fd: i32, buf: *u8, count: uint) -> int

let stdout = 1

fn print(fmt: str) {
	write(stdout, fmt.ptr, fmt.len)
}

fn println(fmt: str) {
	print(fmt)
	print("\n")
}

fn main {
	println("Hello, World!")
	hello()
	exit(answer)
}
