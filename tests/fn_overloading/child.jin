fn extern "c" printf(fmt: *u8, ..) -> i32

fn add*() = {
	unsafe printf("there's nothing to add...\n".ptr)
}

fn add*(a: int, b: int) = {
	unsafe printf("add ints: %d\n".ptr, a + b)
}

fn add*(a: int, b: int, c: int) = {
	unsafe printf("add 3 ints: %d\n".ptr, a + b + c)
}

fn add*(a: f32, b: f32) = {
	unsafe printf("add floats: %f\n".ptr, a + b)
}

fn add*(a: int, b: f32) = {
	unsafe printf("add int & f32: %f\n".ptr, f32(a) + b)
}

fn poly*[T](x: T) = {
	unsafe printf("poly[a]: x = %d\n".ptr, x)
}
