pub(module) fn extern "c" printf(fmt: ptr[u8], ..) -> i32

pub fn add() = {
	unsafe printf("there's nothing to add...\n".data)
}

pub fn add(a: int, b: int) = {
	unsafe printf("add ints: %d\n".data, a + b)
}

pub fn add(a: int, b: int, c: int) = {
	unsafe printf("add 3 ints: %d\n".data, a + b + c)
}

pub fn add(a: f32, b: f32) = {
	unsafe printf("add floats: %f\n".data, a + b)
}

pub fn add(a: int, b: f32) = {
	unsafe printf("add int & f32: %f\n".data, f32(a) + b)
}

pub fn poly[T](x: T) = {
	unsafe printf("poly[a]: x = %d\n".data, x)
}
