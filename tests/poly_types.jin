fn extern printf(fmt: *u8, ..) i32

type Box[a](value: a)

fn main() unit = {
	print(Box(1).value)
	print(Box(3.0).value)
	print(Box(42))
}

fn print(value: int) unit = {
	printf("%d\n".ptr, value)
}

fn print(value: f32) unit = {
	printf("%f\n".ptr, value)
}

fn print(box: &Box[int]) unit = {
	printf("Box(%d)\n".ptr, box.value)
}
