fn extern "c" printf(fmt: *u8, ..) -> i32

fn a() -> u8 = 5
fn b() -> u8 = 9

fn main() = {
    # a = 5(00000101)
    let a: u8 = a()
    # b = 9(00001001)
    let b: u8 = b()
 
    # The result is 00000001
    unsafe printf("a = %d, b = %d\n".data, a, b)
    unsafe printf("a&b = %d\n".data, a & b)
 
    # The result is 00001101
    unsafe printf("a|b = %d\n".data, a | b)
 
    # The result is 00001100
    unsafe printf("a^b = %d\n".data, a ^ b)
 
    # The result is 11111010
    unsafe printf("!a = %d\n".data, !a)
 
    # The result is 00010010
    unsafe printf("b<<1 = %d\n".data, b << 1)
 
    # The result is 00000100
    unsafe printf("b>>1 = %d\n".data, b >> 1)
}
